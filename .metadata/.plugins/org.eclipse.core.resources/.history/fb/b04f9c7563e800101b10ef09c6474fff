import java.awt.*;
import java.awt.event.*;
public class Gui22 
{

	public Gui22() 
	{
		Button b1=new Button("Home");
		b1.setBounds(30,100,80,30);
		add(b1);
		setSize(300,300);
		setLayout(null);
		setVisible(true);
	}

	/**
	 * @param args
	 */
	public static void main(String[] args) {
		new Gui22();

	}

}
