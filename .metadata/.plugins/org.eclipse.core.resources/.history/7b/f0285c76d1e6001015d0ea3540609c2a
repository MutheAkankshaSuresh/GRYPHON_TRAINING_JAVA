package multi;


class multi2
{
	void show()
	{
		System.out.println("Bye");
	}
}

public class multi {

	/**
	 * Multithreading is when one program uses multiple threads 
	 * to perform tasks concurrently while sharing the same resources.
	 */
	public static void main(String[] args) {
		// TODO Auto-generated method stub
		
		for(int i=1;i<=3;i++)
		{
			System.out.println("Hello");
		}

	}

}
