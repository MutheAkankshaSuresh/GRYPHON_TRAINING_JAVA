package Calci;
import java.lang.*;

class Calculate
{
	String name;
	Calculate(String name)
	{
		this.name=name;
	}
	
	public void run()
	{
		calculate_table_5();
		calculate_table_7();
	}
	public synchronized void calculate_table_5()
	{
		System.out.println("Welcome User ");
		for(int i=1;i<=10;i++)
		{
			System.out.println(i*i);
		}
	}
	
	public synchronized void calculate_table_7()
	{
		System.out.println("Welcome User ");
		for(int i=1;i<=10;i++)
		{
			System.out.println(i*i);
		}
	}
}
public class Calci {

	/**
	 * @param args
	 */
	public static void main(String[] args) {
		// TODO Auto-generated method stub
		
      Calculate c1=new Calculate("user1");
      Calculate c1=new Calculate("user2");
      
	}

}
