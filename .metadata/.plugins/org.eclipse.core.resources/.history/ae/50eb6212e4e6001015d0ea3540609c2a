package Calci;
import java.lang.*;

class Print
{
	void show(int n)
	{
		for(int i=1;i<=10;i++)
		{
			System.out.println(n*i);
		}
	}
}

class Table_5
{
	Print F;
	Table_5(Print f)
	{
		F=f;
	}
	
	 void run()
     {
    	 F.show(5); 
     }
}

class Table_7
{
	Print S;
     Table_7(Print s)
     {
    	S=s; 
     }
     
     void run()
     {
    	S.show(7); 
     }
}

public class Calci {

	public static void main(String[] args) {
		
      Print p1=new Print();
      
      Table_5 t1=new Table_5(p1);
      Table_7 t1=new Table_7(p1);
      
      
	}

}
